        <messages/>
        <name>Price Value Changing</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    //Type appropriate comment here, and begin script below
    var cost = g_form.getValue('Cost');
    var percentage = g_form.getValue('Percentage');
    //Now calculating the value of price field
    var price = cost + (cost * (percentage / 100));
    //Setting the calculated value in the price field 
    g_form.setValue('Price', price);

}]]></script>
